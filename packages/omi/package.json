{
  "name": "omi",
  "version": "7.0.0-alpha0",
  "scripts": {
    "start": "vite",
    "dev-vite": "vite",
    "build-vite": "vite build",
    "build-vite:w": "vite build --watch",
    "build": "microbundle",
    "dev": "microbundle watch",
    "server": "live-server ./build --watch",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts,tsx,jsx}' 'test/**/*.{js,ts,tsx,jsx}' 'examples/**/*.{js,ts,tsx,jsx}' --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "doc": "npx typedoc --out docs src"
  },
  "files": [
    "dist",
    "omi.d.ts"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "lint-staged": {
    "**/**.{js,ts}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "construct-style-sheets-polyfill": "3.0.1",
    "weakmap-polyfill": "2.0.4"
  },
  "type": "module",
  "source": "src/index.ts",
  "types": "omi.d.ts",
  "exports": {
    "require": "./dist/omi.cjs",
    "default": "./dist/omi.modern.js"
  },
  "main": "./dist/omi.cjs",
  "module": "./dist/omi.module.js",
  "unpkg": "./dist/omi.umd.js"
}
